{
  "name": "react-portfolio",
  "version": "0.1.1",
  "private": false,
  "homepage": "https://semlak.github.io",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-brands-svg-icons": "^5.6.3",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "axios": "^0.21.1",
    "bootstrap": "^4.1.3",
    "gh-pages": "^2.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-emotion": "^9.2.12",
    "react-phone-number-input": "^2.2.16",
    "react-scripts": "1.1.4",
    "react-snapshot": "^1.3.0",
    "react-spinners": "^0.4.7",
    "reactstrap": "^6.5.0",
    "shortid": "^2.2.13"
